{
    "name": "johnkary/phpunit-speedtrap",
    "description": "Find slow tests in your PHPUnit test suite",
    "keywords": ["PHPUnit", "slow", "profile"],
    "homepage": "https://github.com/johnkary/phpunit-speedtrap",
    "type": "library",
    "license": "MIT",
    "authors": [
        { "name": "John Kary", "email": "john@johnkary.net" }
    ],
    "require": {
        "php": "^7.0",
        "phpunit/phpunit": "^6.0"
    },
    "autoload": {
        "psr-4": {
            "JohnKary\\PHPUnit\\": "src/JohnKary/PHPUnit"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "JohnKary\\PHPUnit\\Tests\\Listener\\": "tests"
        }
    }
}
